{
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "__DEV__": true,
    "describe": true,
    "it": true,
    "before": true,
    "after": true,
    "beforeEach": true,
    "afterEach": true,
    "expect": true,
    "browser": true,
    "by": true,
    "element": true
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "indent": ["error", 2, {"SwitchCase": 1}],
    "eol-last": "off",
    "arrow-body-style": "off",
    "no-underscore-dangle": "off",
    "no-new": 0,
    "max-len": "off",
    "no-console": "off",
    "consistent-return": "off",
    "no-param-reassign": ["error", { "props": false }],
    "no-unused-vars": ["error", { "args": "none" }],
    "no-lonely-if": "off",
    "class-methods-use-this": "error",
    "react/require-extension": "off",
  },
}
